	    LDFLAGS="${LDFLAGS} $ac_i"
	    continue
	fi
    done
		fi
	    fi
	    	    ac_base=`echo $ac_base | sed \
			-e 's/^lib//' 			-e 's/\.a$//' 			-e 's/\.so$//' 			-e 's/\.dylib$//' 			-e 's/\.lib$//' 		    `
	    LIBS="-l${ac_base} ${LIBS_findbak}"
