then :
  withval=$with_f77_libs;
fi

if test "$with_f77_libs" = "yes" -o "$with_f77_libs" = ""; then
        ac_tmp=""
    for ac_i in $FLIBS; do
	# always keep '-lxxx'
	if test "$ac_i" != "`echo $ac_i | sed -e 's/^-l//'`"; then
	    ac_tmp="$ac_tmp $ac_i"
	    continue
	fi
	# always keep non-option items
	# Warning: this breaks cases like '-Ldira -lxxx -Ldirb -lxxx'
	if test "$ac_i" = "`echo $ac_i | sed -e 's/^-//'`"; then
	    ac_tmp="$ac_tmp $ac_i"
	    continue
	fi
	if ! echo " ${LIBS} " " $ac_tmp " | grep " $ac_i " >/dev/null; then
	    ac_tmp="$ac_tmp $ac_i"
	    continue
	fi
    done
    if test -n "$ac_tmp"; then
	if test "append" = "append"; then
	    # append new libs
	    LIBS="${LIBS} $ac_tmp"
	else
	    # prepend new libs
	    LIBS="$ac_tmp ${LIBS}"
	fi
    fi
else
    if test "$with_f77_libs" != "no"; then
	    ac_tmp=""
    for ac_i in $with_f77_libs; do
	# always keep '-lxxx'
	if test "$ac_i" != "`echo $ac_i | sed -e 's/^-l//'`"; then
	    ac_tmp="$ac_tmp $ac_i"
	    continue
	fi
	# always keep non-option items
	# Warning: this breaks cases like '-Ldira -lxxx -Ldirb -lxxx'
	if test "$ac_i" = "`echo $ac_i | sed -e 's/^-//'`"; then
	    ac_tmp="$ac_tmp $ac_i"
	    continue
	fi
	if ! echo " ${LIBS} " " $ac_tmp " | grep " $ac_i " >/dev/null; then
	    ac_tmp="$ac_tmp $ac_i"
	    continue
	fi
    done
    if test -n "$ac_tmp"; then
	if test "append" = "append"; then
	    # append new libs
	    LIBS="${LIBS} $ac_tmp"
	else
	    # prepend new libs
	    LIBS="$ac_tmp ${LIBS}"
	fi
    fi
    fi
fi


