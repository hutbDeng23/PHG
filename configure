  enable_mpi=yes
fi

if test "$enable_mpi" != no; then
    { printf "%s\n" "$as_me:${as_lineno-$LINENO}: checking whether we can include MPI headers " >&5
printf %s "checking whether we can include MPI headers ... " >&6; }
    if test "x$with_mpi_incdir" != x; then
	with_mpi_incdir="-I$with_mpi_incdir"
    fi

    for pass in 0 1; do
	if test $pass = 1 -a "x$with_mpi_incdir" = x; then
	    for d in /usr/include/mpi* /usr/local/mpi*/include \
		/usr/local/mpi*/include /usr/local/include \
		/opt/include /opt/mpi*/include /usr/include/mpi*
	    do
		if test -r ${d}/mpi.h; then
		  with_mpi_incdir="-I$d"
		  break
		fi
	    done
	fi
	CPPFLAGS_bak="${CPPFLAGS}"
	CPPFLAGS="$with_mpi_incdir"
		cat confdefs.h - <<_ACEOF >conftest.$ac_ext
/* end confdefs.h.  */
#include <stdio.h>
		    #include <mpi.h>
		    main() {MPI_Init(0,0); MPI_Finalize();}
_ACEOF
if ac_fn_c_try_compile "$LINENO"
then :
  enable_mpi=yes
else $as_nop
  enable_mpi=no
fi
rm -f core conftest.err conftest.$ac_objext conftest.beam conftest.$ac_ext
	CPPFLAGS="${CPPFLAGS_bak}"
	if test "$enable_mpi" != no -o "x$with_mpi_incdir" != x; then
	    break
	fi
    done
    { printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: $enable_mpi" >&5
printf "%s\n" "$enable_mpi" >&6; }
fi

if test "$enable_mpi" != no; then
    CPPFLAGS_bak="${CPPFLAGS}"
    if test "x$with_mpi_incdir" != x; then
	{ printf "%s\n" "$as_me:${as_lineno-$LINENO}: *** MPI include dir = $with_mpi_incdir" >&5
printf "%s\n" "$as_me: *** MPI include dir = $with_mpi_incdir" >&6;}
	CPPFLAGS="$CPPFLAGS $with_mpi_incdir"
    fi
    { printf "%s\n" "$as_me:${as_lineno-$LINENO}: checking whether we can link with MPI libraries " >&5
printf %s "checking whether we can link with MPI libraries ... " >&6; }
    if test "x$with_mpi_libdir" != x; then
	with_mpi_libdir="-L$with_mpi_libdir"
    fi
    for pass in 0 1; do
	if test $pass = 1 -a "x$with_mpi_libdir" = x; then
	    for d in /usr/lib*/mpi* /usr/lib*/mpi*/lib \
		/usr/local/mpi*/lib /usr/local/lib /usr/lib \
		/opt/lib /opt/mpi*/lib
	    do
		if test -r ${d}/libmpi.a -o -r ${d}/libmpich.a; then
		    #if test "$d" != /usr/lib; then with_mpi_libdir="-L$d"; fi
		    with_mpi_libdir="-L$d"
		    break
		fi
	    done
	fi
	LIBS_bak="${LIBS}"
	for mpi_lib in "$with_mpi_lib" "-lmpi" "-lmpich" "-lmpich -lpmpich"; do
	    LIBS="$with_mpi_libdir $mpi_lib"
	    	    cat confdefs.h - <<_ACEOF >conftest.$ac_ext
/* end confdefs.h.  */
#include <stdio.h>
		    #include <mpi.h>
		    main() {MPI_Init(0,0); MPI_Finalize();}
_ACEOF
if ac_fn_c_try_link "$LINENO"
then :
  enable_mpi=yes; with_mpi_lib=$mpi_lib
else $as_nop
  enable_mpi=no
fi
rm -f core conftest.err conftest.$ac_objext conftest.beam \
    conftest$ac_exeext conftest.$ac_ext
	    if test "$enable_mpi" != no -o "x$with_mpi_lib" != x; then
		break
	    fi
	done
	LIBS="${LIBS_bak}"
	if test "$enable_mpi" != no -o "x$with_mpi_libdir" != x; then
	    break
	fi
    done
    CPPFLAGS="${CPPFLAGS_bak}"
    { printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: $enable_mpi" >&5
printf "%s\n" "$enable_mpi" >&6; }
fi

if test "$enable_mpi" != no; then
    { printf "%s\n" "$as_me:${as_lineno-$LINENO}: *** MPI support enabled" >&5
printf "%s\n" "$as_me: *** MPI support enabled" >&6;}
    if test "x$with_mpi_incdir" != x; then
	{ printf "%s\n" "$as_me:${as_lineno-$LINENO}: *** MPI include dir = $with_mpi_incdir" >&5
printf "%s\n" "$as_me: *** MPI include dir = $with_mpi_incdir" >&6;}
	    for ac_i in $with_mpi_incdir; do
	if test "$ac_i" = "-I/usr/include" -o "$ac_i" = "-L/usr/lib"; then
	    continue
	fi
	if ! echo " ${CPPFLAGS} " | grep " $ac_i " >/dev/null; then
	    CPPFLAGS="${CPPFLAGS} $ac_i"
	    continue
	fi
    done
    fi
    if test "x$with_mpi_libdir" != x; then
	{ printf "%s\n" "$as_me:${as_lineno-$LINENO}: *** MPI lib dir = $with_mpi_libdir" >&5
printf "%s\n" "$as_me: *** MPI lib dir = $with_mpi_libdir" >&6;}
	    for ac_i in $with_mpi_libdir; do
	if test "$ac_i" = "-I/usr/include" -o "$ac_i" = "-L/usr/lib"; then
	    continue
	fi
	if ! echo " ${LDFLAGS} " | grep " $ac_i " >/dev/null; then
	    LDFLAGS="${LDFLAGS} $ac_i"
	    continue
	fi
    done
    fi
    if test "x$with_mpi_lib" != x; then
	{ printf "%s\n" "$as_me:${as_lineno-$LINENO}: *** MPI lib = $with_mpi_lib" >&5
printf "%s\n" "$as_me: *** MPI lib = $with_mpi_lib" >&6;}
	    ac_tmp=""
    for ac_i in $with_mpi_lib; do
	# always keep '-lxxx'
	if test "$ac_i" != "`echo $ac_i | sed -e 's/^-l//'`"; then
	    ac_tmp="$ac_tmp $ac_i"
	    continue
	fi
	# always keep non-option items
	# Warning: this breaks cases like '-Ldira -lxxx -Ldirb -lxxx'
	if test "$ac_i" = "`echo $ac_i | sed -e 's/^-//'`"; then
	    ac_tmp="$ac_tmp $ac_i"
	    continue
	fi
	if ! echo " ${LIBS} " " $ac_tmp " | grep " $ac_i " >/dev/null; then
	    ac_tmp="$ac_tmp $ac_i"
	    continue
	fi
    done
    if test -n "$ac_tmp"; then
	if test "" = "append"; then
	    # append new libs
	    LIBS="${LIBS} $ac_tmp"
	else
	    # prepend new libs
	    LIBS="$ac_tmp ${LIBS}"
	fi
    fi
    fi
    printf "%s\n" "#define USE_MPI 1" >>confdefs.h

    TEST="test"
else
    { printf "%s\n" "$as_me:${as_lineno-$LINENO}: *** MPI support disabled" >&5
printf "%s\n" "$as_me: *** MPI support disabled" >&6;}
    printf "%s\n" "#define USE_MPI 0" >>confdefs.h

    enable_mpiio=no
fi

