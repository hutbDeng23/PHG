    if test "$enable_slepc" = auto; then
	enable_slepc=yes;
    fi
else
    if test "$enable_slepc" = yes; then
	{ printf "%s\n" "$as_me:${as_lineno-$LINENO}: *** --enable-slepc specified but SLEPC_DIR is not set!" >&5
