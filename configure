USE_OMP="0"

OMP_FLAGS=""

# Check whether --enable-omp was given.
if test ${enable_omp+y}
then :
  enableval=$enable_omp;
else $as_nop
  enable_omp="yes"
fi

if test "$enable_omp" != "no"; then
    { printf "%s\n" "$as_me:${as_lineno-$LINENO}: checking for OpenMP support " >&5
printf %s "checking for OpenMP support ... " >&6; }
    tmp="$with_omp_flags"
    if test -z "$tmp"; then
	tmp="-fopenmp -openmp"
    fi
    CFLAGS_bak="$CFLAGS"
    CXXFLAGS_bak="$CXXFLAGS"
    LDFLAGS_bak="$LDFLAGS"
    for with_omp_flags in $tmp; do
	CFLAGS="$CFLAGS_bak"
	CXXFLAGS="$CXXFLAGS_bak"
	LDFLAGS="$LDFLAGS_bak"
	    for ac_i in $with_omp_flags; do
	if test "$ac_i" = "-I/usr/include" -o "$ac_i" = "-L/usr/lib"; then
	    continue
	fi
	if ! echo " ${CFLAGS} " | grep " $ac_i " >/dev/null; then
	    CFLAGS="${CFLAGS} $ac_i"
	    continue
	fi
    done
	    for ac_i in $with_omp_flags; do
	if test "$ac_i" = "-I/usr/include" -o "$ac_i" = "-L/usr/lib"; then
	    continue
	fi
	if ! echo " ${CXXFLAGS} " | grep " $ac_i " >/dev/null; then
	    CXXFLAGS="${CXXFLAGS} $ac_i"
	    continue
	fi
    done
	    for ac_i in $with_omp_flags; do
	if test "$ac_i" = "-I/usr/include" -o "$ac_i" = "-L/usr/lib"; then
	    continue
	fi
	if ! echo " ${LDFLAGS} " | grep " $ac_i " >/dev/null; then
	    LDFLAGS="${LDFLAGS} $ac_i"
	    continue
	fi
    done
	enable_omp="yes"
	cat confdefs.h - <<_ACEOF >conftest.$ac_ext
/* end confdefs.h.  */
#include <omp.h>
		static int dummy;
#pragma omp threadprivate(dummy)
		main() {dummy = omp_get_max_threads();}
_ACEOF
if ac_fn_c_try_link "$LINENO"
then :

else $as_nop
  enable_omp="no"
fi
rm -f core conftest.err conftest.$ac_objext conftest.beam \
    conftest$ac_exeext conftest.$ac_ext
	if test "$enable_omp" != "no"; then break; fi
    done
    if test "$enable_omp" != "no"; then
	{ printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: $with_omp_flags" >&5
printf "%s\n" "$with_omp_flags" >&6; }
	printf "%s\n" "#define USE_OMP 1" >>confdefs.h

	if test "ICC" = "yes"; then
	    MKL_GUIDE="-lguide"
	else
	    MKL_GUIDE="-liomp5"
	fi
    else
	{ printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: no" >&5
printf "%s\n" "no" >&6; }
	CFLAGS="$CFLAGS_bak"
	CXXFLAGS="$CXXFLAGS_bak"
	LDFLAGS="$LDFLAGS_bak"
	MKL_GUIDE="-lguide"
    fi
else
    MKL_GUIDE="-lguide"
fi

if test "ICC" = "yes"; then
    MKL_RT="-lmkl_rt"
else
    if test "$target_cpu" = "x86_64"; then
	MKL_RT="-lmkl_intel_lp64 -lmkl_core -lmkl_gnu_thread"
    else
	MKL_RT="-lmkl_intel -lmkl_core -lmkl_gnu_thread"
    fi
fi

