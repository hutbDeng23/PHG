#--------------------------------------- SuiteSparse

ac_ext=c
ac_cpp='$CPP $CPPFLAGS'
ac_compile='$CC -c $CFLAGS $CPPFLAGS conftest.$ac_ext >&5'
ac_link='$CC -o conftest$ac_exeext $CFLAGS $CPPFLAGS $LDFLAGS conftest.$ac_ext $LIBS >&5'
ac_compiler_gnu=$ac_cv_c_compiler_gnu

# Check whether --enable-ssparse was given.
if test ${enable_ssparse+y}
then :
  enableval=$enable_ssparse;
else $as_nop
  enable_ssparse=yes
fi

if test "$enable_solver" = "no"; then enable_ssparse="no"; fi
if test "$with_float" != "double"; then enable_ssparse="no"; fi
CPPFLAGS_bak="$CPPFLAGS"
LIBS_bak="$LIBS"
LDFLAGS_bak="$LDFLAGS"
if test "$enable_ssparse" != no; then
    { printf "%s\n" "$as_me:${as_lineno-$LINENO}: checking whether we have SuiteSparse solvers " >&5
printf %s "checking whether we have SuiteSparse solvers ... " >&6; }
    if test -z "$with_ssparse_incdir"; then
	for d in /usr/local/SuiteSparse/include /usr/include/suitesparse
	do
	    if test -r ${d}/umfpack.h; then
		with_ssparse_incdir=$d
		break
	    fi
	done
    fi
    if test -z "$with_ssparse_libdir"; then
	if test -r /usr/local/SuiteSparse/lib/libumfpack.a -o \
		-r /usr/local/SuiteSparse/lib/libumfpack.so; then
	    with_ssparse_libdir=/usr/local/SuiteSparse/lib
	fi
    fi
    if test -n "$with_ssparse_incdir"; then
	    for ac_i in -I$with_ssparse_incdir; do
	if test "$ac_i" = "-I/usr/include" -o "$ac_i" = "-L/usr/lib"; then
	    continue
	fi
	if ! echo " ${CPPFLAGS} " | grep " $ac_i " >/dev/null; then
	    CPPFLAGS="${CPPFLAGS} $ac_i"
	    continue
	fi
    done
    fi
    if test -n "$with_ssparse_libdir"; then
	    for ac_i in -L$with_ssparse_libdir; do
	if test "$ac_i" = "-I/usr/include" -o "$ac_i" = "-L/usr/lib"; then
	    continue
	fi
	if ! echo " ${LDFLAGS} " | grep " $ac_i " >/dev/null; then
	    LDFLAGS="${LDFLAGS} $ac_i"
	    continue
	fi
    done
    fi
        ac_tmp=""
    for ac_i in -lumfpack -lamd; do
	# always keep '-lxxx'
	if test "$ac_i" != "`echo $ac_i | sed -e 's/^-l//'`"; then
	    ac_tmp="$ac_tmp $ac_i"
	    continue
	fi
	# always keep non-option items
	# Warning: this breaks cases like '-Ldira -lxxx -Ldirb -lxxx'
	if test "$ac_i" = "`echo $ac_i | sed -e 's/^-//'`"; then
	    ac_tmp="$ac_tmp $ac_i"
	    continue
	fi
	if ! echo " ${LIBS} " " $ac_tmp " | grep " $ac_i " >/dev/null; then
	    ac_tmp="$ac_tmp $ac_i"
	    continue
	fi
    done
    if test -n "$ac_tmp"; then
	if test "" = "append"; then
	    # append new libs
	    LIBS="${LIBS} $ac_tmp"
	else
	    # prepend new libs
	    LIBS="$ac_tmp ${LIBS}"
	fi
    fi
    cat confdefs.h - <<_ACEOF >conftest.$ac_ext
/* end confdefs.h.  */
#include <umfpack.h>
		   main(){umfpack_di_solve (0, 0, 0, 0, 0, 0, 0, 0, 0);}
_ACEOF
if ac_fn_c_try_link "$LINENO"
then :
  enable_ssparse=yes
else $as_nop
  enable_ssparse=no
fi
rm -f core conftest.err conftest.$ac_objext conftest.beam \
    conftest$ac_exeext conftest.$ac_ext
    { printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: $enable_ssparse" >&5
printf "%s\n" "$enable_ssparse" >&6; }
fi
if test "$enable_ssparse" != no; then
    { printf "%s\n" "$as_me:${as_lineno-$LINENO}: *** SuiteSparse solvers enabled" >&5
printf "%s\n" "$as_me: *** SuiteSparse solvers enabled" >&6;}
    printf "%s\n" "#define USE_SSPARSE 1" >>confdefs.h

else
    CPPFLAGS="$CPPFLAGS_bak"
    LIBS="$LIBS_bak"
    LDFLAGS="$LDFLAGS_bak"
    { printf "%s\n" "$as_me:${as_lineno-$LINENO}: *** SuiteSparse solvers disabled" >&5
printf "%s\n" "$as_me: *** SuiteSparse solvers disabled" >&6;}
    printf "%s\n" "#define USE_SSPARSE 0" >>confdefs.h

fi

