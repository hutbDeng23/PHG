    conftest$ac_exeext conftest.$ac_ext
    { printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: $enable_x9amg" >&5
printf "%s\n" "$enable_x9amg" >&6; }
    if test "$enable_x9amg" != "yes"; then
	LIBS="${LIBS_bak}"
	LDFLAGS="${LDFLAGS_bak}"
