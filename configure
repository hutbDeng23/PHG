		    fi
		done
		if test x"${with_papi_incdir}" != x; then break; fi
	    done
	fi
    fi
        if test x"${with_papi_incdir}" = x; then
	for ac_d in  /usr/include/*papi* /usr/*papi*/include 		 /opt/include /opt/include/*papi* /opt/*papi*/include 		 /usr/local/include /usr/local/include/*papi* 		 /usr/local/*papi*/include ${MPI_INCLUDE}/*papi*
	do
	    for ac_f in ${ac_d}/papi.h; do
		if test -r "${ac_f}"; then
		    with_papi_incdir="`dirname ${ac_f}`"
		    break
		fi
	    done
	    if test x"${with_papi_incdir}" != x; then break; fi
	done
    fi
        if test x"${with_papi_lib}" = x; then
	ac_flag=false
	for ac_d in  /usr/lib* /usr/lib*/*papi* /usr/*papi*/lib* 		    /opt/lib* /opt/local/lib* /opt/lib*/*papi* /opt/*papi*/lib* 		    /usr/local/lib* /usr/local/lib*/*papi* /usr/local/*papi*/lib*
	do
	    for ac_n in papi; do
		for ac_f in ${ac_d}/lib${ac_n}.{so,a,dylib,lib}; do
		    if test -r "${ac_f}"; then
			with_papi_lib="${with_papi_lib} `echo ${ac_f}`"
		    fi
		done
	    done
	done
    else
	ac_flag=true
    fi
    CPPFLAGS_findbak="${CPPFLAGS}"
    LDFLAGS_findbak="${LDFLAGS}"
    LIBS_findbak="${LIBS}"
    if test x"${with_papi_incdir}" != x; then
	    for ac_i in -I$with_papi_incdir; do
	if test "$ac_i" = "-I/usr/include" -o "$ac_i" = "-L/usr/lib"; then
	    continue
	fi
	if ! echo " ${CPPFLAGS} " | grep " $ac_i " >/dev/null; then
	    CPPFLAGS="${CPPFLAGS} $ac_i"
	    continue
	fi
    done
    fi
    for ac_lib in "" ${with_papi_lib}; do
	if ${ac_flag}; then
